{
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "global": {},
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "modules": true
    }
  },
  "rules": {
    "no-console": 0,
  },
}
